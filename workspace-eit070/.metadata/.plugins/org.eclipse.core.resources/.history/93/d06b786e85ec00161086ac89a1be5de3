/* Initial exercise to understand signed variables

char b;

int main() {
	b=9;
	while(1==1){
		if(b<10){
			b--;
		}
	}
}
*/

/* Understanding difference of unsigned and signed char

unsigned char b;

int main() {
	b = 9;
	while (1 == 1) {
		if (b < 10) {
			b--;
		}
	}
}
*/

/* Understanding the signed int

int b;

int main() {
	b = 9;
	while (1 == 1) {
		if (b < 10) {
			b--;
		}
	}
}
*/

int b;

int main() {
	b = 9;
	while (1 == 1) {
		if (b < 10) {
			b--;
		}
	}
}
